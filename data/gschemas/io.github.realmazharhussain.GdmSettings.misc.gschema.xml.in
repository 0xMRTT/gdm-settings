<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="@application_id@.misc" path="@schema_path@misc/">
    
    <key name="enable-welcome-message" type="b">
      <default>false</default>
    </key>

    <key name="welcome-message" type="s">
      <default>"Welcome!"</default>
    </key>

    <key name="enable-logo" type="b">
      <default>false</default>
    </key>

    <key name="logo" type="s">
      <default>""</default>
    </key>

    <key name="disable-restart-buttons" type="b">
      <default>false</default>
    </key>

    <key name="disable-user-list" type="b">
      <default>false</default>
    </key>

  </schema>
</schemalist>
