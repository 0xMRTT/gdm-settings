#: data/application_id.appdata.xml.in:76
msgid "Support for setting manual verbosity level"
msgstr "Unterstützung für das manuelle Setzen eines Detaillevels"

#: data/application_id.appdata.xml.in:77
msgid "Command-line options for verbose/qeiet mode and manual verbosity level"
msgstr ""
"Kommandozeilenoptionen für einen detaillierten, stillen und normalen Modus"

#: data/application_id.appdata.xml.in:78
msgid "Command-line option --version"
msgstr "Kommandozeilenoption --version"

#: data/application_id.appdata.xml.in:79
