    <property name="website-label">Homepage</property>
    <property name="license-type">GTK_LICENSE_AGPL_3_0</property>
    <property name="copyright">Copyright Â© 2021 Mazhar Hussain</property>
    <property name="authors">Mazhar Hussain &lt;mmazharhussainkgb1145@gmail.com&gt;</property>
    <property name="artists">Mazhar Hussain &lt;mmazharhussainkgb1145@gmail.com&gt;</property>
    <property name="documenters">Mazhar Hussain &lt;mmazharhussainkgb1145@gmail.com&gt;</property>
  </object>
